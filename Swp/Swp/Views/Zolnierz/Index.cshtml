@model IEnumerable<Swp.Model.Zolnierz>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Stopien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Imie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Naziwsko)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Adres)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Imieojca)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Imiematki)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pesel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numertelefonu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdgrupyNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @{@Html.DisplayFor(modelItem => item.Stopien)}
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Imie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Naziwsko)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adres)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Imieojca)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Imiematki)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pesel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numertelefonu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdgrupyNavigation.Idgrupy)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idzolnierza">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idzolnierza">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idzolnierza">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
